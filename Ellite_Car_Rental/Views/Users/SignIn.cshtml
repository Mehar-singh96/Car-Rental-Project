@model Ellite_Car_Rental.Models.Login

@{
    ViewBag.Title = "SignIn";
}
<link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet" crossorigin="anonymous">


<h2 class="display-1">Sign In</h2>

@ViewBag.error


<style>

    #email_{




    }

    #pass_{



    }



</style>




@using (Html.BeginForm("SignInAfter", "Users"))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        <hr />
        <div class="form-group">

            <div class="form-group">
                <label for="email" id="email_">Email</label>
                <input id="email" name="email" class="form-control" aria-describedby="emailHelp" placeholder="Enter email" />
                <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
            </div>


            <div class="form-group">
                <label for="pass" id="pass_">Password</label>
                <input id="pass" name="pass" placeholder="Password" class="form-control" type="password" />

            </div>


            <input type="submit" value="Sign In" class="btn btn-primary" onclick="IsEmpty();" />
        </div>
    </div>
}


<div>
    @*@Html.ActionLink("sign In", "SignInAfter"); *@
</div>


<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

<script language="Javascript">
    function IsEmpty() {

        if (document.form.question.value == "") {
            alert("empty");
        }
        return;
    }
</script>